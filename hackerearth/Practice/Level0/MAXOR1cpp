#include<bits/stdc++.h>
using namespace std;
#define test() int tt;cin>>tt;cin.get();for(int test=1;test<=tt;test++)
#define boost() ios::sync_with_stdio(false)cin.tie(0);cout.tie(0);

int main(){
boost();

    int n;
    cin>>n;
    vector <int> v;
    for(int i=0;i<n;i++){
    	int a;
    	cin>>a;
    	v.push_back(a);

    }

	int ans=0;
		do{
			for(int i=0;i<n;i++){
				int ans1=v[0],ans2=v[i];
				for(int j=1;j<i;j++){

					ans1=max(ans1,ans1^v[j]);
				}
				for(int j=i+1;j<n;j++){
					ans2=max(ans2,ans2^v[j]);

				}
				ans=max(ans,ans1^ans2);
			}

		}while(next_permutation(v.begin(),v.end()));

		cout<<ans<<endl;
}
